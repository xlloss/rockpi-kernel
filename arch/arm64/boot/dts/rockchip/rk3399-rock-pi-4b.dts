/dts-v1/;

#include "rk3399-rock-pi-4.dtsi"

/ {

	model = "ROCK PI 4B SLASH V01";
	compatible = "rockchip,rockpi","rockchip,rk3399";

	sample-gpio-led {
		status = "okay";
		compatible = "sample,gpio-led";
		led0-gpios = <&gpio4 4 GPIO_ACTIVE_HIGH>;
		led1-gpios = <&gpio4 6 GPIO_ACTIVE_HIGH>;
		led2-gpios = <&gpio4 7 GPIO_ACTIVE_HIGH>;
		led3-gpios = <&gpio4 8 GPIO_ACTIVE_HIGH>;
	};

};

&wireless_wlan {
	status = "okay";
};

&wireless_bluetooth {
	status = "okay";
};
